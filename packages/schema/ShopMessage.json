{
    "$id": "shopMessage.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "ShopMessage",
    "type": "object",
    "properties": {
        "shopName": {
            "type": "string"
        },
        "date": {
            "type": "string",
            "format": "date"
        },
        "inStock": {
            "type": "boolean"
        },
        "price": {
            "type": "number"
        }
    },
    "required": [
        "shopName",
        "date",
        "inStock",
        "price"
    ]
}